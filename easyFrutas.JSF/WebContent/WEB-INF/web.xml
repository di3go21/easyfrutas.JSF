<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<request-character-encoding>UTF-8</request-character-encoding>
	<display-name>easyFrutas.es</display-name>
	<welcome-file-list>
		<welcome-file>HOME</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>ControladorPrincipal</servlet-name>
		<servlet-class>controller.ControladorPrincipal</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Registro</servlet-name>
		<servlet-class>controller.SignUp</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ControladorPrincipal</servlet-name>
		<url-pattern>/HOME</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Registro</servlet-name>
		<url-pattern>/Registro</url-pattern>
	</servlet-mapping>



</web-app>